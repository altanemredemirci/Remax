@model List<ResultAgencyDTO>
<!-- Team Start -->
<div class="container-xxl py-5">
    <div class="container">
        <div class="text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 600px;">
            <h1 class="mb-3">Acenteler</h1>
            <p>Kendi alanında tecrübeli ortaklarımız ile kaliteli bir hizmet anlayışı.</p>
        </div>
        <div class="row g-4">
            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                    <div class="team-item rounded overflow-hidden">
                        <div class="position-relative">
                            <img class="img-fluid" src="~/realestate/img/@item.ImageUrl" alt="">
                            <div class="position-absolute start-50 top-100 translate-middle d-flex align-items-center">
                                <a class="btn btn-square mx-1" href="@item.FacebookAddress"><i class="fab fa-facebook-f"></i></a>
                                <a class="btn btn-square mx-1" href="@item.XAddress"><i class="fab fa-twitter"></i></a>
                                <a class="btn btn-square mx-1" href="@item.InstagramAddress"><i class="fab fa-instagram"></i></a>
                            </div>
                        </div>
                        <div class="text-center p-4 mt-3">
                            <h5 class="fw-bold mb-0">@item.Name</h5>
                            <small>@item.Title</small>
                        </div>
                    </div>
                </div>
            }
            
        </div>
    </div>
</div>
<!-- Team End -->
